<view class="container"><view class="header" style="z-index:10000;"><view class="title-container"><text class="title">{{fieldName+''}}<view class="_br"></view></text><text class="fieldDescription">{{fieldDescription}}</text></view></view><view class="map-container" id="map"></view><view data-event-opts="{{[['tap',[['closeTab',['$event']]]]]}}" class="{{['mask',(isTabShow)?'mask-show':'']}}" bindtap="__e"></view><view class="{{['side-tab',(isTabShow)?'side-tab-show':'']}}"><view class="tab-content"><text class="tab-title">click to change<view class="_br"></view>the map</text><view class="map-icons"><image class="map-icon" src="/static/area/ai.svg" mode="aspectFit"></image><image class="map-icon" src="/static/area/biology.svg" mode="aspectFit"></image><image class="map-icon" src="/static/area/math.svg" mode="aspectFit"></image></view></view></view><view class="bottom-bg"><image src="/static/docker/梯形.svg" mode="aspectFill"></image></view><view class="bottom-docker"><view data-event-opts="{{[['tap',[['navigateTo',['map']]]]]}}" class="trapezoid t1" bindtap="__e"><image class="docker-icon" style="z-index:20;margin-left:40rpx;" src="/static/docker/地图.svg" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['navigateTo',['daily']]]]]}}" class="trapezoid t2" bindtap="__e"><image class="docker-icon" style="z-index:20;margin-left:40rpx;" src="/static/docker/日推.svg" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['navigateTo',['achievement']]]]]}}" class="trapezoid t3" bindtap="__e"><image class="docker-icon" style="z-index:20;margin-left:40rpx;" src="/static/docker/成就.svg" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['navigateTo',['profile']]]]]}}" class="trapezoid t4" bindtap="__e"><image class="docker-icon" style="z-index:20;margin-left:40rpx;" src="/static/docker/我.svg" mode="aspectFit"></image></view></view><image class="mascot" style="{{'width:'+('480rpx')+';'+('height:'+('480rpx')+';')+('position:'+('fixed')+';')+('left:'+(mascotLeft+'px')+';')+('bottom:'+('60rpx')+';')+('transition:'+(isDragging?'none':'left 0.3s ease-out')+';')}}" src="{{mascotSrc}}" mode="aspectFill" data-event-opts="{{[['touchstart',[['handleTouchStart',['$event']]]],['touchmove',[['handleTouchMove',['$event']]]],['touchend',[['handleTouchEnd',['$event']]]],['tap',[['toggleMascot',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e" bindtap="__e"></image></view>